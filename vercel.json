{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/tournament/*",
      "destination": "/tournament/[id]/index.html"
    },
    {
      "source": "/update-tournament/*",
      "destination": "/update-tournament/[id]/index.html"
    }
  ]
}